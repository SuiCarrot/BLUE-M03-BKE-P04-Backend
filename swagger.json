            "post": {
              "summary": "Cadastrar Usuário",
              "description": "Rota responsável por cadastrar um usuário",
              "tags": ["Users"],
              "requestBody": {
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object"
                    },
                    "examples": {
                      "Usuário": {
                        "value": {
                          "nameUser": "Rick Sanche",  
                          "username": "",
                          "email": "",
                          "password": "",
                          "photo": "https://rickandmortyapi.com/api/character/avatar/1.jpeg"
                        }
                      }
                    }
                  }
                }
              },
              "responses": {
                "400": {
                  "description": "Bad Request"
                },
                "201": {
                  "description": "Created",
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "object"
                        }
                      }
                    }
                  }
                }
              }
            },
            "/auth/login": {
                "post": {
                  "summary": "Realizar Login",
                  "description": "Rota responsável por logar o usuário",
                  "tags": ["Auth"],
                  "requestBody": {
                    "content": {
                      "application/json": {
                        "schema": {
                          "type": "object"
                        },
                        "examples": {
                          "Usuário": {
                            "value": {
                              "email": "",
                              "password": ""
                            }
                          }
                        }
                      }
                    }
                  },
                  "responses": {
                    "400": {
                      "description": "Bad Request"
                    },
                    "201": {
                      "description": "Created",
                      "content": {
                        "application/json": {
                          "schema": {
                            "type": "object"
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
